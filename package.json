        "yup": "0.29.0"
    },
    "devDependencies": {
        "@types/jest": "^26.0.21",
        "@types/node": "^14.14.35",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.2",
        "copyfiles": "2.1.1",
        "electron-devtools-installer": "^3.1.1",
        "husky": "4",
        "jest": "^26.6.3",
        "jsdoc": "^3.6.6",
        "node-sass": "4.13.0",
        "prettier": "1.19.1",
        "serve": "11.2.0",
        "typescript": "^4.2.3",
