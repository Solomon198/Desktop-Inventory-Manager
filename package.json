        "webpack-messages": "2.0.4",
        "webpack-node-externals": "^2.5.2",
        "webpack-rtl-plugin": "2.0.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn lint-fix ; yarn format",
            "pre-push": "yarn lint-fix ; yarn format"
        }
    }
}
