        "react-scripts": "3.2.0",
        "react-select": "3.1.0",
        "react-swipeable-views": "0.13.9",
        "react-syntax-highlighter": "12.2.1",
        "react-window": "1.8.5",
        "realm": "^10.2.0",
        "redux": "4.0.5",
        "redux-persist": "6.0.0",
