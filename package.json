{
    "name": "inventory-management-system",
    "version": "1.0.0",
    "private": true,
    "homepage": "./",
    "main": "public/electron.js",
    "scripts": {
        "start:web:dev": "yarn run concurrently \"react-scripts start\" \"yarn \" ",
        "start:desktop:dev:windows": " start yarn compile:tsc && concurrently \"yarn craco start\"  \"wait-on http://localhost:3000 && electron . --no-sandbox \"",
        "start:desktop:dev:linux":"yarn craco start & wait-on http://localhost:3000 && electron . --no-sandbox",
        "docs":" yarn run jsdoc src -r -c ./conf.json -d docs",
        "build": "react-scripts build",
        "build:desktop:prod": "craco build",
        "test": "craco test",
        "lint": "eslint src",
        "compile:tsc": "yarn tsc --noEmit false",
        "lint-fix": "eslint --fix src",
        "format": "prettier --write \"src/**/*.{js,css,scss,html}\"",
        "format-watch": "onchange \"src/**/*.{js,css,scss,html}\" prettier --write \"src/**/*.{js,css,scss,html}\"",
        "rtl": "webpack"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all",
            "ie 11"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version",
            "last 1 edge version",
            "IE 11"
        ]
    },
    "dependencies": {
        "@craco/craco": "5.8.0",
        "@date-io/date-fns": "2.6.1",
        "@formatjs/intl-pluralrules": "1.3.5",
        "@fortawesome/fontawesome-free": "5.13.0",
        "@manaflair/redux-batch": "1.0.0",
        "@material-ui/core": "4.9.14",
        "@material-ui/icons": "4.9.1",
        "@material-ui/lab": "4.0.0-alpha.53",
        "@material-ui/pickers": "3.2.10",
        "@material-ui/styles": "4.9.14",
        "@reduxjs/toolkit": "1.3.6",
        "@tanem/svg-injector": "8.0.50",
        "apexcharts": "3.19.2",
        "aws-sdk": "^2.863.0",
        "axios": "0.19.2",
        "axios-mock-adapter": "1.18.1",
        "bootstrap": "4.6.0",
        "clipboard-copy": "3.1.0",
        "clsx": "1.1.0",
        "concurrently": "^6.0.0",
        "cp-cli": "2.0.0",
        "css-mediaquery": "0.1.2",
        "date-fns": "2.8.1",
        "downshift": "3.4.2",
        "electron": "^12.0.1",
        "electron-builder": "^22.10.5",
        "electron-is-dev": "^2.0.0",
        "fg-loadcss": "2.1.0",
        "formik": "2.1.4",
