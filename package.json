        "perfect-scrollbar": "1.5.0",
        "prop-types": "15.7.2",
        "react": "16.12.0",
        "react-bootstrap": "1.0.1",
        "react-bootstrap-table-next": "4.0.2",
        "react-bootstrap-table2-paginator": "2.1.2",
        "react-datepicker": "2.16.0",
        "react-dom": "16.12.0",
        "react-draggable": "4.4.2",
        "react-inlinesvg": "1.2.0",
        "react-intl": "3.6.2",
